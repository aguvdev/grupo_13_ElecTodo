<!DOCTYPE html>
<html lang="en">
    <%- include('../partials/head') %> 
<body>
    <%- include('../partials/header') %>
    <main class="profile">
        <section class="perfil-usuario">
            <div class="contenedor-perfil">
                <div class="fondo-perfil-oscuro">
                    <div class="logo-carnet">
                        <img src="/img/electodo log.png" alt="">
                    </div>
                    <div class="foto-perfil">
                        <img src="/img/<%= usuarios.image %> " alt="">
                        <div class="efecto-borde"></div>
                    </div>
                    <div class="fondo-perfil-claro">
                        <div class="datos-usuario">
                        <div class="nombre-usuario">
                            <label class="nombre-apellido"><%= usuarios.nombre %> <%= usuarios.lastname %></label>
                            <label for="">Nombre y Apellido </label>
                        </div>
                        <div class="datos-generales">
                            <p class="numero-cel">011-4444-5555</p>
                            <label>Numero de celular</label>

                            <p class="email"><%= usuarios.email %> </p>
                            <label>Correo Electrónico</label>

                            <p id="passOculta" class="email"><%= usuarios.password %> </p>
                            <label>Contraseña</label>
                        </div>
                        <button>Editar</button>
                    </div>
                    </div>
                </div>
            </div>
        </section>
    </main>
    <%- include('../partials/footer') %>
    <%- include('../partials/scripts') %>
</body>
</html>